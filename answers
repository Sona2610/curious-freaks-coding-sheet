#1
num=int(input("Enter A Number:"))
if num%2==0:
    print("EVEN NUMBER")
else:
    print("ODD NUMBER")
num = int(input())
while num != 0:
    if  num% 2 == 0:
        print("even")
    else:
        print("odd")
    num=0

